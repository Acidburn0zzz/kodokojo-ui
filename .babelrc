{
  "env": {
    "development": {
      "presets": ["es2015", "react", "stage-1", "react-hmre"],
      "plugins": [
        "transform-object-rest-spread"
      ]
    },
    "test": {
      "presets": ["es2015", "react", "stage-1"],
      "plugins": [
        "transform-object-rest-spread",
        "babel-plugin-rewire"
      ]
    },
    "production": {
      "presets": ["es2015", "react", "stage-1"],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  }
}